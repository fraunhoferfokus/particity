<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
  <compatibility>
    <version>6.2.0+</version>
  </compatibility>
  <theme id="ah_theme" name="Adhoc Engagement Theme">
    <template-extension>ftl</template-extension>
    <settings>
      <setting key="portlet-setup-show-borders-default" value="false"></setting>
      <setting
        configurable="true"
        key="show-site-name-default"
        type="checkbox"
        value="false"
    />
    <setting
        configurable="true"
        key="show-site-name-supported"
        type="checkbox"
        value="true"
    />
    </settings>
    
    
  </theme>
</look-and-feel>